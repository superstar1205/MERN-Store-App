{
  "name": "my-store",
  "version": "1.0.0",
  "node-version": "14.16.0",
  "description": "Online Store Management System",
  "private": true,
  "main": "./server/server.js",
  "scripts": {
    "start": "babel-node server/server.js",
    "clean": "rimraf node_modules && rimraf dist && npm install",
    "webpack:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.dev.js",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "build": "rimraf dist && npm run webpack:dev && npm run start",
    "build:prod": "rimraf dist && npm run webpack:prod && npm run start",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "lint": "eslint client server; exit 0",
    "docs": "jsdoc -c jsdoc.conf.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/serviceWorker.js",
      "!**/index.js",
      "!src/app/plugins/*.{js,jsx}",
      "!src/app/assets/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@hapi/joi": "^17.1.1",
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@mui/styles": "^5.3.0",
    "axios": "^0.21.4",
    "better-docs": "^2.3.2",
    "body-parser": "^1.19.1",
    "bookshelf": "^1.2.0",
    "checksum": "^1.0.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "connected-react-router": "^6.9.2",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^5.0.0-alpha.7",
    "formik": "^2.2.9",
    "helmet": "^4.6.0",
    "history": "^4.10.1",
    "html2canvas": "^1.4.0",
    "http-status-codes": "^2.2.0",
    "i18next": "^21.6.5",
    "i18next-browser-languagedetector": "^6.1.2",
    "jest-canvas-mock": "^2.3.1",
    "jsdoc": "^3.6.9",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "knex": "^0.95.15",
    "md5": "^2.3.0",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.4",
    "mysql": "^2.17.1",
    "path": "^0.12.7",
    "q": "^1.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.15.3",
    "react-lines-ellipsis": "^0.15.0",
    "react-moment": "^1.1.1",
    "react-paypal-express-checkout": "^1.0.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-devtools": "^3.7.0",
    "redux-form": "^8.3.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "serve-favicon": "^2.5.0",
    "styled-components": "^5.3.3",
    "swagger-jsdoc": "^4.3.2",
    "tar-pack": "^3.4.1",
    "underscore": "^1.13.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-function-bind": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.16.7",
    "@hot-loader/react-dom": "^17.0.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^1.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest-enzyme": "^7.1.2",
    "husky": "^6.0.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.15",
    "node-sass": "^7.0.1",
    "prettier": "^2.5.1",
    "parcel-bundler": "^1.12.5",
    "parser": "^0.1.4",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-dev-server": "^3.11.3",
    "webpack-hot-middleware": "^2.25.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./client/**/**/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --trailing-comma es5 --single-quote --write"
    ]
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.8"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:learning-zone/my-store.git"
  },
  "keywords": [
    "node",
    "es6",
    "express",
    "react",
    "redux",
    "react-router",
    "webpack",
    "boilerplate"
  ],
  "author": "Pradeep Kumar <pradeep.vwa@gmail.com>",
  "license": "MIT"
}
